   SELECT PI.CODPROD AS CODIGO,PT.DESCRICAO AS PRODUTO,PI.CODENDERECO AS ENDERECO,PE.DEPOSITO,PE.RUA,PE.PREDIO,PE.NIVEL,PE.APTO,PI.NUMINVENT AS NUMERO_INVENTARIO,PI.DTINCLUSAO AS DATA_INVENTARIO,PI.MATRICULA,PER.NOME FROM PCINVENTITENS PI
      
      FULL JOIN PCINVENTENDERECOIVOLUME PVI
       ON PI.CODPROD = PVI.CODPROD 
       
       INNER JOIN PCPRODUT PT
       ON PI.CODPROD = PT.CODPROD 
       
       INNER JOIN PCENDERECO PE
       ON PI.CODENDERECO = PE.CODENDERECO
       
       FULL JOIN PCEMPR PER
       ON PI.MATRICULA = PER.MATRICULA
       
       where pi.codprod is not null
        
       order by pi.dtinclusao desc ;
