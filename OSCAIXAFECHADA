 SELECT DISTINCT PT.CLIENTE,PP.NUMPED,(PM.NUMOS),PM.DTFIMOS,(PV.NUMVOL),PP.ROTA,PT.MUNICCOB AS CIDADE,pt.bairrocob AS BAIRRO,PP.VLTABELA AS VALOR_PEDIDO,PM.QTSEPARADA,PM.CODPROD,PDT.DESCRICAO,peo.estacao ||'-'|| PR.NOME,PM.TIPOOS FROM PCPEDC PP
        
        INNER JOIN PCMOVENDPEND PM
       ON PP.NUMPED = PM.NUMPED
       
       INNER JOIN PCVOLUMEOS PV
       ON PM.NUMOS = PV.NUMOS
       
       INNER JOIN PCPRACA PP
       ON PM.CODPRACA = PP.CODPRACA
       
       INNER JOIN PCCLIENT PT
       ON PP.CODCLI = PT.CODCLI
       
       INNER JOIN PCPRODUT PDT
       ON PM.CODPROD = PDT.CODPROD
       
       inner join pcendereco peo
       on pm.codendereco = peo.codendereco
       
       INNER JOIN PCEMPR PR
       ON PM.CODFUNCOS = PR.MATRICULA
        
         WHERE PP.DATA = :data AND PP.ROTA BETWEEN 1 AND 89 AND PM.TIPOOS = 20
        ORDER BY PP.DATA DESC ;
